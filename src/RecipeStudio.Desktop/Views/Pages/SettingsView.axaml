<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="RecipeStudio.Desktop.Views.Pages.SettingsView">

  <Grid ColumnDefinitions="290,*" Margin="18">

    <Border Grid.Column="0" Classes="card" Padding="12">
      <DockPanel>
        <StackPanel DockPanel.Dock="Top" Spacing="10">
          <TextBlock Text="–ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–∏—Å—Ç–µ–º—ã" Classes="h1" />
          <TextBlock Text="–ù–∞–≤–∏–≥–∞—Ü–∏—è –ø–æ —Ä–∞–∑–¥–µ–ª–∞–º" Classes="muted" />

          <TreeView SelectionChanged="SettingsTree_SelectionChanged">
            <TreeViewItem Header="–°–∏—Å—Ç–µ–º–∞" IsExpanded="True">
              <TreeViewItem Header="–•—Ä–∞–Ω–∏–ª–∏—â–µ" Tag="storage" />
              <TreeViewItem Header="–ü–∞—Ä–∞–º–µ—Ç—Ä—ã —Å—Ç–∞–Ω–∫–∞" Tag="machine" />
              <TreeViewItem Header="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç—ã" Tag="coefficients" />
              <TreeViewItem Header="–ì—Ä–∞—Ñ–∏–∫–∞" Tag="graphics" />
            </TreeViewItem>
          </TreeView>
        </StackPanel>

        <StackPanel DockPanel.Dock="Bottom" Spacing="8" Margin="0,16,0,0">
          <TextBlock Text="–§–∞–π–ª—ã –ø—Ä–∏–ª–æ–∂–µ–Ω–∏—è" FontWeight="SemiBold" />
          <StackPanel Spacing="2">
            <TextBlock Text="–§–∞–π–ª –Ω–∞—Å—Ç—Ä–æ–µ–∫:" Classes="muted tiny" />
            <TextBlock Text="{Binding SettingsFilePath}" TextWrapping="Wrap" ToolTip.Tip="–ü—É—Ç—å –∫ settings.json" />
          </StackPanel>
          <StackPanel Spacing="2">
            <TextBlock Text="–§–∞–π–ª –ë–î —Ä–µ—Ü–µ–ø—Ç–æ–≤:" Classes="muted tiny" />
            <TextBlock Text="{Binding DatabaseFilePath}" TextWrapping="Wrap" ToolTip.Tip="–ü—É—Ç—å –∫ SQLite –±–∞–∑–µ recipes.sqlite" />
          </StackPanel>
        </StackPanel>
      </DockPanel>
    </Border>

    <Grid Grid.Column="1" Margin="12,0,0,0" RowDefinitions="Auto,*,Auto">
      <Border Grid.Row="0" Classes="card">
        <Grid ColumnDefinitions="*,Auto" VerticalAlignment="Center">
          <TextBlock Text="{Binding CurrentSectionTitle}" FontSize="20" FontWeight="Bold" VerticalAlignment="Center" />
          <Button Grid.Column="1" Command="{Binding SaveCommand}" Classes="accent" ToolTip.Tip="–°–æ—Ö—Ä–∞–Ω–∏—Ç—å –≤—Å–µ –∏–∑–º–µ–Ω–µ–Ω–∏—è">–°–æ—Ö—Ä–∞–Ω–∏—Ç—å</Button>
        </Grid>
      </Border>

      <ScrollViewer Grid.Row="1">
        <StackPanel Spacing="12">

          <Border Classes="card" IsVisible="{Binding IsStorageSection}">
            <StackPanel Spacing="12">
              <TextBlock Text="–•—Ä–∞–Ω–∏–ª–∏—â–µ —Ä–µ—Ü–µ–ø—Ç–æ–≤" FontSize="16" FontWeight="SemiBold" />
              <TextBlock Text="–î–≤–æ–π–Ω–æ–π –∫–ª–∏–∫ –ø–æ –ø–æ–ª—é –ø—É—Ç–∏ –æ—Ç–∫—Ä–æ–µ—Ç –∫–∞—Ç–∞–ª–æ–≥ –≤ –ø—Ä–æ–≤–æ–¥–Ω–∏–∫–µ" Classes="muted" />

              <Grid ColumnDefinitions="*,Auto,Auto" RowDefinitions="Auto,Auto">
                <TextBlock Grid.Row="0" Grid.ColumnSpan="3" Text="–ü–∞–ø–∫–∞ —Ö—Ä–∞–Ω–µ–Ω–∏—è" Classes="muted" />

                <TextBox Grid.Row="1"
                         Grid.Column="0"
                         Text="{Binding RecipesFolder}"
                         PointerPressed="RecipesFolder_PointerPressed"
                         ToolTip.Tip="–ü—É—Ç—å –∫ –ø–∞–ø–∫–µ —Å —Ä–µ—Ü–µ–ø—Ç–∞–º–∏ –∏ SQLite –±–∞–∑–æ–π. –î–≤–æ–π–Ω–æ–π –∫–ª–∏–∫ –æ—Ç–∫—Ä—ã–≤–∞–µ—Ç –∫–∞—Ç–∞–ª–æ–≥" />

                <Button Grid.Row="1"
                        Grid.Column="1"
                        Click="OpenFolder_Click"
                        Classes="ghost"
                        Margin="8,0,0,0"
                        Width="40"
                        Height="34"
                        FontSize="16"
                        ToolTip.Tip="–û—Ç–∫—Ä—ã—Ç—å —Ç–µ–∫—É—â–∏–π –∫–∞—Ç–∞–ª–æ–≥ —Ä–µ—Ü–µ–ø—Ç–æ–≤">
                  üìÇ
                </Button>

                <Button Grid.Row="1"
                        Grid.Column="2"
                        Click="BrowseFolder_Click"
                        Classes="ghost"
                        Margin="8,0,0,0"
                        Width="40"
                        Height="34"
                        FontSize="16"
                        ToolTip.Tip="–í—ã–±—Ä–∞—Ç—å –¥—Ä—É–≥—É—é –ø–∞–ø–∫—É —Ä–µ—Ü–µ–ø—Ç–æ–≤">
                  üóÅ
                </Button>
              </Grid>
            </StackPanel>
          </Border>

          <Border Classes="card" IsVisible="{Binding IsMachineSection}">
            <StackPanel Spacing="14">
              <TextBlock Text="–ü–∞—Ä–∞–º–µ—Ç—Ä—ã —Å—Ç–∞–Ω–∫–∞" FontSize="16" FontWeight="SemiBold" />

              <Grid ColumnDefinitions="*,*">
                <Border Classes="card" Padding="10">
                  <StackPanel Spacing="8">
                    <TextBlock Text="–ì–µ–æ–º–µ—Ç—Ä–∏—è –∏ —Å–º–µ—â–µ–Ω–∏—è" FontWeight="SemiBold" />
                    <Grid ColumnDefinitions="220,*" RowDefinitions="Auto,Auto,Auto,Auto">
                      <TextBlock Text="–°–º–µ—â–µ–Ω–∏–µ X (Xm)" VerticalAlignment="Center" ToolTip.Tip="–°–º–µ—â–µ–Ω–∏–µ –ø–æ –æ—Å–∏ X, –º–º" />
                      <TextBox Grid.Column="1" Text="{Binding Xm}" Margin="10,0,0,0" ToolTip.Tip="–°–º–µ—â–µ–Ω–∏–µ –ø–æ –æ—Å–∏ X, –º–º" />

                      <TextBlock Grid.Row="1" Text="–°–º–µ—â–µ–Ω–∏–µ Y (Ym)" VerticalAlignment="Center" ToolTip.Tip="–°–º–µ—â–µ–Ω–∏–µ –ø–æ –æ—Å–∏ Y, –º–º" />
                      <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding Ym}" Margin="10,0,0,0" ToolTip.Tip="–°–º–µ—â–µ–Ω–∏–µ –ø–æ –æ—Å–∏ Y, –º–º" />

                      <TextBlock Grid.Row="2" Text="–°–º–µ—â–µ–Ω–∏–µ Z (Zm)" VerticalAlignment="Center" ToolTip.Tip="–°–º–µ—â–µ–Ω–∏–µ –ø–æ –æ—Å–∏ Z, –º–º" />
                      <TextBox Grid.Row="2" Grid.Column="1" Text="{Binding Zm}" Margin="10,0,0,0" ToolTip.Tip="–°–º–µ—â–µ–Ω–∏–µ –ø–æ –æ—Å–∏ Z, –º–º" />

                      <TextBlock Grid.Row="3" Text="–î–ª–∏–Ω–∞ —Å–æ–ø–ª–∞ (Lz)" VerticalAlignment="Center" ToolTip.Tip="–î–ª–∏–Ω–∞ —Å–æ–ø–ª–∞, –º–º" />
                      <TextBox Grid.Row="3" Grid.Column="1" Text="{Binding Lz}" Margin="10,0,0,0" ToolTip.Tip="–î–ª–∏–Ω–∞ —Å–æ–ø–ª–∞, –º–º" />
                    </Grid>
                  </StackPanel>
                </Border>

                <Border Grid.Column="1" Classes="card" Padding="10" Margin="12,0,0,0">
                  <StackPanel Spacing="8">
                    <TextBlock Text="–ë–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç—å –∏ –∑–æ–Ω—ã" FontWeight="SemiBold" />
                    <Grid ColumnDefinitions="220,*" RowDefinitions="Auto,Auto,Auto">
                      <TextBlock Text="–í—ã—Å–æ—Ç–∞ –ø–µ—Ä–µ—Ö–æ–¥–∞ (H_cont)" VerticalAlignment="Center" ToolTip.Tip="–ë–µ–∑–æ–ø–∞—Å–Ω–∞—è –≤—ã—Å–æ—Ç–∞ –ø–µ—Ä–µ—Ö–æ–¥–∞, –º–º" />
                      <TextBox Grid.Column="1" Text="{Binding HContMax}" Margin="10,0,0,0" ToolTip.Tip="–ë–µ–∑–æ–ø–∞—Å–Ω–∞—è –≤—ã—Å–æ—Ç–∞ –ø–µ—Ä–µ—Ö–æ–¥–∞, –º–º" />

                      <TextBlock Grid.Row="1" Text="–ó–æ–Ω–∞ –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏ (H_zone)" VerticalAlignment="Center" ToolTip.Tip="–ú–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è —Ä–∞–±–æ—á–∞—è –∑–æ–Ω–∞ –ø–æ –≤—ã—Å–æ—Ç–µ, –º–º" />
                      <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding HZone}" Margin="10,0,0,0" ToolTip.Tip="–ú–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è —Ä–∞–±–æ—á–∞—è –∑–æ–Ω–∞ –ø–æ –≤—ã—Å–æ—Ç–µ, –º–º" />

                      <TextBlock Grid.Row="2" Text="–ë–æ–∫–æ–≤–æ–π –ø—Ä–µ–¥–µ–ª (H_bok)" VerticalAlignment="Center" ToolTip.Tip="–ë–æ–∫–æ–≤–æ–µ –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∏–µ, –º–º" />
                      <TextBox Grid.Row="2" Grid.Column="1" Text="{Binding HBokMax}" Margin="10,0,0,0" ToolTip.Tip="–ë–æ–∫–æ–≤–æ–µ –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∏–µ, –º–º" />
                    </Grid>
                  </StackPanel>
                </Border>
              </Grid>
            </StackPanel>
          </Border>

          <Border Classes="card" IsVisible="{Binding IsCoefficientsSection}">
            <StackPanel Spacing="12">
              <TextBlock Text="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç—ã (vpulse)" FontSize="16" FontWeight="SemiBold" />
              <TextBlock Text="–ò–º–ø—É–ª—å—Å–æ–≤ –Ω–∞ –º–º. –ü–∞—Ä–∞–º–µ—Ç—Ä—ã –∏—Å–ø–æ–ª—å–∑—É—é—Ç—Å—è –¥–ª—è –ø–µ—Ä–µ—Å—á—ë—Ç–∞ —Ç—Ä–∞–µ–∫—Ç–æ—Ä–∏–π." Classes="muted" />

              <Grid ColumnDefinitions="*,*,*,*" RowDefinitions="Auto,Auto,Auto,Auto">
                <TextBlock Text="VPULSE_X" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ X" />
                <TextBlock Grid.Column="1" Margin="12,0,0,0" Text="VPULSE_Y" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ Y" />
                <TextBlock Grid.Column="2" Margin="12,0,0,0" Text="VPULSE_Z" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ Z" />
                <TextBlock Grid.Column="3" Margin="12,0,0,0" Text="VPULSE_A" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ A" />

                <TextBox Grid.Row="1" Text="{Binding PulseX}" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ X" />
                <TextBox Grid.Row="1" Grid.Column="1" Margin="12,0,0,0" Text="{Binding PulseY}" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ Y" />
                <TextBox Grid.Row="1" Grid.Column="2" Margin="12,0,0,0" Text="{Binding PulseZ}" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ Z" />
                <TextBox Grid.Row="1" Grid.Column="3" Margin="12,0,0,0" Text="{Binding PulseA}" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ A" />

                <TextBlock Grid.Row="2" Text="VPULSE_B" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ B" />
                <TextBlock Grid.Row="2" Grid.Column="1" Margin="12,0,0,0" Text="VPULSE_TOP" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –≤–µ—Ä—Ö–Ω–µ–≥–æ –º–µ—Ö–∞–Ω–∏–∑–º–∞" />
                <TextBlock Grid.Row="2" Grid.Column="2" Margin="12,0,0,0" Text="VPULSE_LOW" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –Ω–∏–∂–Ω–µ–≥–æ –º–µ—Ö–∞–Ω–∏–∑–º–∞" />
                <TextBlock Grid.Row="2" Grid.Column="3" Margin="12,0,0,0" Text="VPULSE_CLAMP" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∑–∞–∂–∏–º–∞" />

                <TextBox Grid.Row="3" Text="{Binding PulseB}" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∏–º–ø—É–ª—å—Å–æ–≤ –ø–æ –æ—Å–∏ B" />
                <TextBox Grid.Row="3" Grid.Column="1" Margin="12,0,0,0" Text="{Binding PulseTop}" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –≤–µ—Ä—Ö–Ω–µ–≥–æ –º–µ—Ö–∞–Ω–∏–∑–º–∞" />
                <TextBox Grid.Row="3" Grid.Column="2" Margin="12,0,0,0" Text="{Binding PulseLow}" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –Ω–∏–∂–Ω–µ–≥–æ –º–µ—Ö–∞–Ω–∏–∑–º–∞" />
                <TextBox Grid.Row="3" Grid.Column="3" Margin="12,0,0,0" Text="{Binding PulseClamp}" ToolTip.Tip="–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç –∑–∞–∂–∏–º–∞" />
              </Grid>
            </StackPanel>
          </Border>

          <Border Classes="card" IsVisible="{Binding IsGraphicsSection}">
            <StackPanel Spacing="12">
              <TextBlock Text="–ü–∞—Ä–∞–º–µ—Ç—Ä—ã –≥—Ä–∞—Ñ–∏–∫–∏" FontSize="16" FontWeight="SemiBold" />

              <Grid ColumnDefinitions="240,*,70" RowDefinitions="Auto,Auto,Auto,Auto">
                <TextBlock Text="–ü—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å –ª–∏–Ω–∏–∏" VerticalAlignment="Center" ToolTip.Tip="–ü—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å –∫—Ä–∏–≤–æ–π –Ω–∞ –≥—Ä–∞—Ñ–∏–∫–µ (0.05‚Äì0.90)" />
                <Slider Grid.Column="1" Minimum="0.05" Maximum="0.90" Value="{Binding PlotOpacity}" Margin="10,0,10,0" ToolTip.Tip="–ü—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å –∫—Ä–∏–≤–æ–π –Ω–∞ –≥—Ä–∞—Ñ–∏–∫–µ (0.05‚Äì0.90)" />
                <TextBlock Grid.Column="2" Text="{Binding PlotOpacityDisplay}" VerticalAlignment="Center" HorizontalAlignment="Right" />

                <TextBlock Grid.Row="1" Text="–¢–æ–ª—â–∏–Ω–∞ –ª–∏–Ω–∏–∏" VerticalAlignment="Center" ToolTip.Tip="–¢–æ–ª—â–∏–Ω–∞ –æ—Å–Ω–æ–≤–Ω–æ–π –ª–∏–Ω–∏–∏ –≥—Ä–∞—Ñ–∏–∫–∞" />
                <TextBox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Text="{Binding PlotStrokeThickness}" Margin="10,0,0,0" ToolTip.Tip="–¢–æ–ª—â–∏–Ω–∞ –æ—Å–Ω–æ–≤–Ω–æ–π –ª–∏–Ω–∏–∏ –≥—Ä–∞—Ñ–∏–∫–∞" />

                <TextBlock Grid.Row="2" Text="–†–∞–¥–∏—É—Å —Ç–æ—á–µ–∫" VerticalAlignment="Center" ToolTip.Tip="–†–∞–∑–º–µ—Ä –æ—Ç–æ–±—Ä–∞–∂–∞–µ–º—ã—Ö —Ç–æ—á–µ–∫ –Ω–∞ –≥—Ä–∞—Ñ–∏–∫–µ" />
                <TextBox Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2" Text="{Binding PlotPointRadius}" Margin="10,0,0,0" ToolTip.Tip="–†–∞–∑–º–µ—Ä –æ—Ç–æ–±—Ä–∞–∂–∞–µ–º—ã—Ö —Ç–æ—á–µ–∫ –Ω–∞ –≥—Ä–∞—Ñ–∏–∫–µ" />

                <TextBlock Grid.Row="3" Text="–°–µ–≥–º–µ–Ω—Ç—ã —Å–≥–ª–∞–∂–∏–≤–∞–Ω–∏—è" VerticalAlignment="Center" ToolTip.Tip="–ß–µ–º –≤—ã—à–µ –∑–Ω–∞—á–µ–Ω–∏–µ, —Ç–µ–º –±–æ–ª–µ–µ –ø–ª–∞–≤–Ω–∞—è –∫—Ä–∏–≤–∞—è –∏ –≤—ã—à–µ –Ω–∞–≥—Ä—É–∑–∫–∞" />
                <TextBox Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="2" Text="{Binding SmoothSegmentsPerSpan}" Margin="10,0,0,0" ToolTip.Tip="–ß–µ–º –≤—ã—à–µ –∑–Ω–∞—á–µ–Ω–∏–µ, —Ç–µ–º –±–æ–ª–µ–µ –ø–ª–∞–≤–Ω–∞—è –∫—Ä–∏–≤–∞—è –∏ –≤—ã—à–µ –Ω–∞–≥—Ä—É–∑–∫–∞" />
              </Grid>

              <CheckBox IsChecked="{Binding PlotShowPolyline}" ToolTip.Tip="–ü–æ–∫–∞–∑—ã–≤–∞—Ç—å –ª–æ–º–∞–Ω—É—é –ª–∏–Ω–∏—é –∏—Å—Ö–æ–¥–Ω—ã—Ö —Ç–æ—á–µ–∫">–ü–æ–∫–∞–∑—ã–≤–∞—Ç—å –ª–æ–º–∞–Ω—É—é (polyline)</CheckBox>
              <CheckBox IsChecked="{Binding PlotShowSmooth}" ToolTip.Tip="–ü–æ–∫–∞–∑—ã–≤–∞—Ç—å —Å–≥–ª–∞–∂–µ–Ω–Ω—É—é –∫—Ä–∏–≤—É—é">–ü–æ–∫–∞–∑—ã–≤–∞—Ç—å —Å–≥–ª–∞–∂–µ–Ω–Ω—É—é –ª–∏–Ω–∏—é (spline)</CheckBox>
              <CheckBox IsChecked="{Binding PlotShowTargetPoints}" ToolTip.Tip="–ü–æ–∫–∞–∑—ã–≤–∞—Ç—å —Ü–µ–ª–µ–≤—ã–µ —Ç–æ—á–∫–∏ —Ä–µ—Ü–µ–ø—Ç–∞">–ü–æ–∫–∞–∑—ã–≤–∞—Ç—å —Ç–æ—á–∫–∏</CheckBox>
              <CheckBox IsChecked="{Binding PlotEnableDrag}" ToolTip.Tip="–†–∞–∑—Ä–µ—à–∏—Ç—å –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏–µ —Ç–æ—á–µ–∫ –º—ã—à—å—é –Ω–∞ –≥—Ä–∞—Ñ–∏–∫–µ">–†–∞–∑—Ä–µ—à–∏—Ç—å –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏–µ —Ç–æ—á–µ–∫ –Ω–∞ –≥—Ä–∞—Ñ–∏–∫–µ</CheckBox>
            </StackPanel>
          </Border>
        </StackPanel>
      </ScrollViewer>

      <Border Grid.Row="2" Classes="card">
        <TextBlock Text="–ü–æ–¥—Å–∫–∞–∑–∫–∞: –Ω–∞–≤–µ–¥–∏—Ç–µ –∫—É—Ä—Å–æ—Ä –Ω–∞ –∏–∫–æ–Ω–∫–∏ –∏ –ø–∞—Ä–∞–º–µ—Ç—Ä—ã, —á—Ç–æ–±—ã —É–≤–∏–¥–µ—Ç—å –Ω–∞–∑–Ω–∞—á–µ–Ω–∏–µ." Classes="muted" />
      </Border>
    </Grid>

  </Grid>
</UserControl>
